generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id             Int     @id @default(autoincrement())
  name           String  @unique @db.VarChar(255)
  type1          String  @db.VarChar(255)
  type2          String? @db.VarChar(255)
  species        String  @db.VarChar(255)
  height         String  @db.VarChar(255)
  weight         String  @db.VarChar(255)
  abilities      String  @db.VarChar(255)
  evYield        String  @db.VarChar(255)
  catchRate      String  @db.VarChar(255)
  baseFriendship String  @db.VarChar(255)
  baseExp        Int
  growthRate     String  @db.VarChar(255)
  eggGroups      String  @db.VarChar(255)
  gender         String  @db.VarChar(255)
  eggCycles      String  @db.VarChar(255)
  hpBase         Int
  hpMin          Int
  hpMax          Int
  attackBase     Int
  attackMin      Int
  attackMax      Int
  defenseBase    Int
  defenseMin     Int
  defenseMax     Int
  spAttackBase   Int
  spAttackMin    Int
  spAttackMax    Int
  spDefenseBase  Int
  spDefenseMin   Int
  spDefenseMax   Int
  speedBase      Int
  speedMin       Int
  speedMax       Int
}

model pokemon {
  id             Int     @id @default(autoincrement())
  name           String  @unique(map: "name") @db.VarChar(255)
  type1          String  @db.VarChar(100)
  type2          String? @db.VarChar(100)
  species        String? @db.VarChar(255)
  height         String? @db.VarChar(100)
  weight         String? @db.VarChar(100)
  abilities      String? @db.VarChar(500)
  evYield        String? @db.VarChar(255)
  catchRate      String? @db.VarChar(255)
  baseFriendship String? @db.VarChar(100)
  baseExp        Int?
  growthRate     String? @db.VarChar(100)
  eggGroups      String? @db.VarChar(255)
  gender         String? @db.VarChar(100)
  eggCycles      String? @db.VarChar(100)
  hpBase         Int?
  hpMin          Int?
  hpMax          Int?
  attackBase     Int?
  attackMin      Int?
  attackMax      Int?
  defenseBase    Int?
  defenseMin     Int?
  defenseMax     Int?
  spAttackBase   Int?
  spAttackMin    Int?
  spAttackMax    Int?
  spDefenseBase  Int?
  spDefenseMin   Int?
  spDefenseMax   Int?
  speedBase      Int?
  speedMin       Int?
  speedMax       Int?
}
